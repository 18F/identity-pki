# This file is used by the Figaro gem: https://github.com/laserlemon/figaro

# Be sure to restart your server when you modify this file.

# Make sure the secrets in this file are kept private
# if you're sharing your code publicly.

# Make sure any new entries you add are enclosed in single quotes.
# Figaro requires all values to be explicit strings.

production:
  allow_third_party_auth: '<%= @allow_third_party_auth %>'
  domain_name: '<%= @domain_name %>'
  enable_test_routes: '<%= @enable_test_routes %>'
  google_analytics_key: '<%= @google_analytics_key %>'
  idp_sso_target_url: '<%= @idp_sso_target_url %>'
  idv_attempt_window_in_hours: '<%= @idv_attempt_window_in_hours %>'
  idv_max_attempts: '<%= @idv_max_attempts %>'
  logins_per_ip_limit: '<%= @logins_per_ip_limit %>'
  logins_per_ip_period: '<%= @logins_per_ip_period %>'
  mailer_domain_name: '<%= @mailer_domain_name %>'
  newrelic_license_key: '<%= @newrelic_license_key %>'
  otp_delivery_blocklist_bantime: '<%= @otp_delivery_blocklist_bantime %>'
  otp_delivery_blocklist_findtime: '<%= @otp_delivery_blocklist_findtime %>'
  otp_delivery_blocklist_maxretry: '<%= @otp_delivery_blocklist_maxretry %>'
  participate_in_dap: '<%= @participate_in_dap %>'
  proofing_vendors: '<%= @proofing_vendors %>'
  recovery_code_length: '<%= @recovery_code_length %>'
  redis_url: '<%= @redis_url %>'
  requests_per_ip_limit: '<%= @requests_per_ip_limit %>'
  requests_per_ip_period: '<%= @requests_per_ip_period %>'
  saml_passphrase: '<%= @saml_passphrase %>'
  secret_key_base: '<%= @secret_key_base %>'
  session_timeout_in: '<%= @session_timeout_in %>'
  # Set the number of seconds in which to delay the start of the
  # PeriodicalQuery() call. Make sure the sum of this value and
  # session_timeout_warning_seconds is a multiple of 60 seconds.
  session_check_delay: '<%= @session_check_delay %>'
  # Set the frequency of the PeriodicalQuery() call in seconds.
  # Make sure the sum of this value and session_timeout_warning_seconds
  # is a multiple of 60 seconds.
  session_check_frequency: '<%= @session_check_frequency %>'
  # Set the number of seconds the timeout warning should occur before
  # login session is timed out.
  session_timeout_warning_seconds: '<%= @session_timeout_warning_seconds %>'
  smtp_settings: '<%= @smtp_settings %>'
  stale_session_window: '<%= @stale_session_window %>'
  support_email: '<%= @support_email %>'
  support_url: '<%= @support_url %>'
  twilio_accounts: '<%= @twilio_accounts %>'
  valid_service_providers: '<%= @valid_service_providers %>'
