name: Invalid User
type: any
index: logstash-*
alert_text: Somebody tried to log in as an invalid user.
owner: <%= @env %>
filter:
- query:
    query_string:
      query: '"Attempt to login using a non-existent user"'
alert:
<% if ! @emails.empty? %>
- "email"
<% end %>
<% if ! @webhook.nil? %>
- "slack"
<% end %>
<% if ! @emails.empty? %>
email:
<%   @emails.each do |e| %>
- "<%= e %>"
<%   end %>
<% end %>
<% if ! @webhook.nil? %>
slack_webhook_url: <%= @webhook %>
slack_username_override: <%= "#{@env} alerts" %>
slack_channel_override: <%= @slackchannel %>
<% end %>

