output {
   s3 {
     region => "<%= @aws_region %>"
     bucket => "<%= @aws_logging_bucket %>"
     time_file => 5
     server_side_encryption => true
     tags => <%= @tags %>
     prefix => "elk/%{+YYYY}-%{+MM}-%{+dd}/"
     proxy_uri => "<%= @proxy_uri -%>"
     validate_credentials_on_root_bucket => false
   }
}
