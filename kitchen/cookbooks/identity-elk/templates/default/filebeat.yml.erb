---
filebeat.config.inputs:
  enabled: true
  path: inputs.d/*.yml

output.logstash.hosts:
<% @logstash_hosts.each do |hosts| -%>
<%= "- #{hosts}" -%>
<% end -%>

output.logstash.ssl.certificate_authorities:
<% @logstash_ssl_certificate_authorities.each do |ca| -%>
<%= "- #{ca}" -%>
<% end %>

queue.spool:
  file:
    size: 4000MiB
    page_size: 16KiB
