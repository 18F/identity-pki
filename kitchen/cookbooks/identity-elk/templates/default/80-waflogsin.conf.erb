input {
   s3 {
     bucket => "<%= @waf_logging_bucket %>"
     proxy_uri => "<%= @proxy_uri -%>"
     region => "<%= @aws_region %>"
     sincedb_path => "/usr/share/logstash/.sincedb_waf"
     type => "waf"
   }
}

filter {
  if [type] == "waf" {
     json {
      source => "message"
      target => "waf"
    }
  }
}
