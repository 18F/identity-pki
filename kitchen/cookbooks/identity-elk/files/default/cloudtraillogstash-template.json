{
  "template": "cloudtraillogstash",
  "index_patterns": ["logstash-cloudtrail-*"],
  "settings": {
    "index": {
      "mapping": {
        "total_fields": {
          "limit": "3000"
        }
      }
    }
  },
  "mappings": {
    "doc": {
      "properties": {
        "apiVersion": {
          "type": "keyword"
        }
      }
    }
  }
}
