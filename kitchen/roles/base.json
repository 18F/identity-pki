{
  "name": "base",
  "description": "",
  "json_class": "Chef::Role",
  "default_attributes": {
    "authorization": {
      "sudo": {
        "groups": [
          "sudo"
        ],
        "passwordless": true
      }
    },
    "chef_proxy_env": {
      "config_files": {
        "http_proxy": "/etc/login.gov/info/http_proxy",
        "https_proxy": "/etc/login.gov/info/http_proxy",
        "no_proxy": "/etc/login.gov/info/no_proxy_hosts"
      }
    }
  },
  "chef_type": "role",
  "run_list": [
    "recipe[chef_proxy_env]",
    "recipe[instance_certificate::generate]",
    "recipe[service_discovery::register]",
    "recipe[login_dot_gov::configure_awsagent]",
    "recipe[login_dot_gov::configure_clamav]",
    "recipe[login_dot_gov::chage]",
    "recipe[login_dot_gov::ssh]",
    "recipe[login_dot_gov::users]",
    "recipe[apt]",
    "recipe[bash-completion]",
    "recipe[build-essential]",
    "recipe[cacert]",
    "recipe[git]",
    "recipe[hostname]",
    "recipe[motd]",
    "recipe[identity_base_config]",
    "recipe[ssh-keys]",
    "recipe[sudo]",
    "recipe[identity-elk::logcleanup]",
    "recipe[identity-elk::filebeat]",
    "recipe[login_dot_gov::resolve]",
    "recipe[identity_base_config::ruby]",
    "recipe[identity-monitoring]"
  ],
  "env_run_lists": {
  }
}
